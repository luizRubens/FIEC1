create database si_tarde;
 use si_tarde;
 show tables;

create table aluno (
cod_aluno int not null auto_increment primary key,
nome varchar (100) not null);

insert into aluno (nome) values ("Luiz");

insert into aluno (nome) values ("João");

insert into aluno (nome) values ("Ana");


